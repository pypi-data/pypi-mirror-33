### IMPORTANT ###
# Copy this file to db.local before customising for your application unless
# you will be providing a custom location to toputils.db. This is so that 
# your configurations will not be overwritten every time that the package 
# is reinstalled.

[test_db]
user = user-name
password = password1
database = my-database
host = 0.0.0.0
charset = utf8
ssl_ca = /dir/to/ca.pem
ssl_cert = /dir/to/client-cert.pem
ssl_key = /dir/to/client-key.pem
