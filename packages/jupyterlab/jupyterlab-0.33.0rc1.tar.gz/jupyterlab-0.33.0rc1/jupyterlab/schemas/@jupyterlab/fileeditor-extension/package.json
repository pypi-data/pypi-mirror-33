{
  "name": "@jupyterlab/fileeditor-extension",
  "version": "0.17.0-1",
  "description": "JupyterLab - Editor Widget Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "license": "BSD-3-Clause",
