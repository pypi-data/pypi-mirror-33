    "@jupyterlab/console": "^0.17.0-1",
    "@jupyterlab/coreutils": "^2.0.0-1",
    "@jupyterlab/docregistry": "^0.17.0-1",
    "@jupyterlab/filebrowser": "^0.17.0-1",
    "@jupyterlab/fileeditor": "^0.17.0-1",
    "@jupyterlab/launcher": "^0.17.0-1",
    "@jupyterlab/mainmenu": "^0.6.0-1",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/widgets": "^1.6.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~2.9.2"
