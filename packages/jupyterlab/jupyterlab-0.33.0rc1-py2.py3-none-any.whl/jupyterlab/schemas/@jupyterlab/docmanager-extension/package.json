    "@jupyterlab/docregistry": "^0.17.0-1",
    "@jupyterlab/mainmenu": "^0.6.0-1",
    "@jupyterlab/services": "^3.0.0-1",
    "@phosphor/disposable": "^1.1.2"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~2.9.2"
