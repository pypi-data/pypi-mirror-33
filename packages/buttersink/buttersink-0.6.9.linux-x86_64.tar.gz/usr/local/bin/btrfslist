#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'buttersink==0.6.9','console_scripts','btrfslist'
__requires__ = 'buttersink==0.6.9'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('buttersink==0.6.9', 'console_scripts', 'btrfslist')()
    )
