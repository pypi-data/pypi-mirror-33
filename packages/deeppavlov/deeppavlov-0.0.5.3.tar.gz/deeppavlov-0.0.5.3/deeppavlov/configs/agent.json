{
  "chainer": {
    "in": ["text", "id"],
    "pipe": [
      {
        "in": ["text", "id"],
        "out": ["response"],
        "class": "deeppavlov.core.agent:Agent",
        "skills": [
          {
            "class": "deeppavlov.skills.pattern_matching_skill:PatternMatchingSkill",
            "responses": ["Hello world!"],
            "patterns": ["hi", "hello", "good day"]
          },
          {
            "class": "deeppavlov.skills.pattern_matching_skill:PatternMatchingSkill",
            "responses": ["Goodbye world!", "See you around"],
            "patterns": ["bye", "chao", "see you"]
          },
          {
            "class": "deeppavlov.skills.pattern_matching_skill:PatternMatchingSkill",
            "responses": ["I don't understand, sorry", "I can say \"Hello world!\""]
          }
        ],
        "skills_selector": {
          "class": "deeppavlov.core.agent:HighestConfidenceSelector"
        }
      }
    ],
    "out": ["response"]
  }
}