#!/Users/jess/.local/conda/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'earthchem==0.0.5','console_scripts','echem'
__requires__ = 'earthchem==0.0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('earthchem==0.0.5', 'console_scripts', 'echem')()
    )
