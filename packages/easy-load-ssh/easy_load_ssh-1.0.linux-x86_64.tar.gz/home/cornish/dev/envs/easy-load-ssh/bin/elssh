#!/home/cornish/dev/envs/easy-load-ssh/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'easy-load-ssh==1.0','console_scripts','elssh'
__requires__ = 'easy-load-ssh==1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('easy-load-ssh==1.0', 'console_scripts', 'elssh')()
    )
