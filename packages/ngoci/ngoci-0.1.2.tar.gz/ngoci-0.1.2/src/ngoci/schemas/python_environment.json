{
  "$schema": "http://numengo.org/draft-04/schema",
  "$id": "http://numengo.org/ngoci/python_environment",
  "title": "PythonEnvironment",
  "type": "object",
  "extends": ["http://numengo.org/ngoci/environment"],
  "additionalProperties": false,
  "properties": {
    "name": {
        "type": "string",
