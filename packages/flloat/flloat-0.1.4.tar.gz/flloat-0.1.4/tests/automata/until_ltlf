	fake [style=invisible]
	0 [root=true]
	1 [shape=doublecircle]
	fake -> 0 [style=bold]
	0 -> 1 [label="{B}"]
	0 -> 1 [label="{A, B, C}"]
	0 -> 0 [label="{A}"]
	0 -> 1 [label="{A, B}"]
	0 -> 0 [label="{A, C}"]
	0 -> 1 [label="{B, C}"]
	1 -> 1 [label="{B}"]
	1 -> 1 [label="{A, B, C}"]
	1 -> 1 [label="{A}"]
	1 -> 1 [label="{C}"]
	1 -> 1 [label="{A, B}"]
	1 -> 1 [label="{}"]
	1 -> 1 [label="{B, C}"]
	1 -> 1 [label="{A, C}"]
	label="until
A U B"
