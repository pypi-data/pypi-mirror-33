digraph {
	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1
	fake -> 0 [style=bold]
	0 -> 0 [label="{B}"]
	0 -> 1 [label="{A, B, C}"]
	0 -> 1 [label="{A}"]
	0 -> 0 [label="{C}"]
	0 -> 1 [label="{A, B}"]
	0 -> 0 [label="{}"]
	0 -> 0 [label="{B, C}"]
	0 -> 1 [label="{A, C}"]
	1 -> 0 [label="{B}"]
	1 -> 1 [label="{A, B}"]
	1 -> 0 [label="{B, C}"]
	1 -> 1 [label="{A, B, C}"]
	label="always implication and next
G (A -> X(B) )"
	fontsize=20
}
