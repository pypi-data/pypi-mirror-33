component KappaFunc() in -> out {
    run : flow.Deselect()
    a : flow.Init(data=0)
    b : flow.Init(data=1)
    fib : math.Compute(op="+")

    .in > run.select
    a.out > run.case_true
    voidport > run.case_false
    run.data > fib.a
    b.out > fib.b
    fib.result > b.in
    b.out > a.in
    fib.result > .out
}

f : KappaFunc()
out : io.Print()

true > f.in

f.out > out.token
